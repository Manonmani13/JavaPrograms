 } else if(Objects.equal(status, "active")) {
                            String config = bulkUserDAO.getValue("select description from organization_config where config='htuUsers'");
                            if(!Objects.equal(config,null)) {
                                String getMode = bulkUserDAO.getValue("select mode from alumnis where id="+alumniLogs.getAlumniId());
                                String ids = bulkUserDAO.getValue("select user_id from alumnis where id="+alumniLogs.getAlumniId());
                                Long id = Long.valueOf(ids);

                                if (getMode.equalsIgnoreCase("registration")) {
                                    String action = "alumniAcknowledgement";
                                   pushToAlumniQueueHtu(action, id, authToken.getOrganizationId(), alumniLogs.getAlumniId());
                                }
                                else
                                {
                                    String action = "alumniAccountActivation";
                                    pushToAlumniQueueHtu(action, id, authToken.getOrganizationId(), alumniLogs.getAlumniId());
                                }
                            }
