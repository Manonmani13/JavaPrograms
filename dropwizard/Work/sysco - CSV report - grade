select ap.id,s.user_id,cam.short_name campus, ap.occurrence_course_code as program, concat(s.first_name, ' ', s.last_name) student, s.user_id,gs.status as grade from user_program_unit_mapping um 
                 inner join student_program_unit_mapping sm on um.program_id=sm.academic_program_id and um.mapping_id=sm.mapping_id and um.academic_year_id=sm.academic_year_id 
                 inner join students s on s.user_id=sm.user_id inner join masterlookup cam on cam.id=s.campus 
                 inner join academic_program ap on ap.id=sm.academic_program_id  
                 inner join grading_user_unit_status gs on gs.user_id = sm.user_id  
                 inner join `course_staff_mapping` `csm` ON `csm`.`academic_program_id` = `ap`.`id` and csm.manager_id = 200431
 where sm.status=1 and um.unit_id= 48 and um.program_id=120 and um.academic_year_id = 70985
