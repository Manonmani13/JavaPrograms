 public JSONObject studentInfos(Long id, AuthToken authToken) throws NoSuchAlgorithmException, KeyManagementException, ParseException {
        JSONObject jo = new JSONObject();
        Map<String, Object>  details =enrolmentUnityPushLogDAO.getmap("select * from vw_biw_civa_appplication_push where Application No="+id);
//1
        CivicaApplicationDetails civicaApplicationDetails = new CivicaApplicationDetails();

        LinkedHashMap personalInfo=new LinkedHashMap<>();
        JSONArray info = new JSONArray();
        personalInfo.put("emailId",details.get("Email_Id"));
        personalInfo.put("firstName",details.get("Legal_First_Name"));
        personalInfo.put("middleName",details.get("Middle_Name"));
        personalInfo.put("surName",details.get("Surname"));
        personalInfo.put("gender",details.get("Gender"));
        personalInfo.put("dateOfBirth",details.get("Date_of_birth"));
        personalInfo.put("age",details.get("Age"));
        personalInfo.put("zipCode",details.get("Post_Code"));
        personalInfo.put("homeAddress",details.get("Home_Address"));
        personalInfo.put("mobileNumber",details.get("Mobile_Number"));
        personalInfo.put("homeTelephoneNumber",details.get("Home_Telephone_Number"));
        personalInfo.put("ethnicity",details.get("Ethnicity"));
        personalInfo.put("address",details.get("Have_you_lived_in_the_uk_for_the_last_three_years"));
        info.add(personalInfo);
        jo.put("personalInformation",info);

//2
        JSONArray emergency1 = new JSONArray();
        LinkedHashMap emergencyContact1=new LinkedHashMap<>();
        emergencyContact1.put("title",details.get("Emergency_contact1_title"));
        emergencyContact1.put("firstName",details.get("Emergency_contact1_first_name"));
        emergencyContact1.put("surName",details.get("Emergency_contact1_surname"));
        emergencyContact1.put("homeTelephoneNumber",details.get("Emergency_contact1home_telephone_number"));
        emergencyContact1.put("zipCode",details.get("Emergency_contact1_address"));
        emergencyContact1.put("address",details.get("Emergency_contact1_post_code"));
        emergencyContact1.put("workTelephoneNumber",details.get("Emergency_contact1_work_telephone_number"));
        emergencyContact1.put("emailAddress",details.get("Emergency_contact1_email_address"));
        emergencyContact1.put("mobileNumber",details.get("Emergency_contact1_contact_receive_your_progress_information"));
        emergencyContact1.put("progressInformation",details.get("EducationList"));
        emergencyContact1.put("whoIsContact",details.get("Who_is_contact"));
        emergency1.add(emergencyContact1);
        jo.put("EmergencyContactList",emergency1);
//3
        JSONArray emergency2 = new JSONArray();
        LinkedHashMap emergencyContact2=new LinkedHashMap<>();
        emergencyContact2.put("title",details.get("Emergency_contact2_title"));
        emergencyContact2.put("firstName",details.get("Emergency_contact2_first_name"));
        emergencyContact2.put("surName",details.get("Emergency_contact2_surname"));
        emergencyContact2.put("homeTelephoneNumber",details.get("Emergency_contact2home_telephone_number"));
        emergencyContact2.put("zipCode",details.get("Emergency_contact2_address"));
        emergencyContact2.put("address",details.get("Emergency_contact2_post_code"));
        emergencyContact2.put("workTelephoneNumber",details.get("Emergency_contact2_work_telephone_number"));
        emergencyContact2.put("emailAddress",details.get("Emergency_contact2_email_address"));
        emergencyContact2.put("mobileNumber",details.get("Emergency_contact2_mobile_number"));
        emergencyContact2.put("progressInformation",details.get("Emergency_contact2_contact_receive_your_progress_information"));
        emergencyContact2.put("whoIsContact",details.get("Who_is_contact2"));
        emergencyContact2.put("school",details.get("school"));
        emergency2.add(emergencyContact2);
        jo.put("EmergencyContactList",emergency2);
//4

        JSONArray application = new JSONArray();
        LinkedHashMap submitApplication=new LinkedHashMap<>();
        submitApplication.put("medicalCondition",details.get("Medical_Condition"));
        submitApplication.put("supportAtSchool",details.get("Support_At_School"));
        submitApplication.put("examProvision",details.get("Special_Exam_Provision"));
        submitApplication.put("specialArrangements",details.get("Special_Arrangements"));
        submitApplication.put("healthCare",details.get("Arrangements_Making"));
        submitApplication.put("safeGuarding",details.get("safeguaring"));
        submitApplication.put("supportNeeds",details.get("Support_Need"));
        submitApplication.put("socialServices",details.get("Social_Services"));
        submitApplication.put("submittedDate",details.get("Submitted_Date"));
        application.add(submitApplication);
        jo.put("Application",application);
//5
        JSONArray course = new JSONArray();
        LinkedHashMap courseDetails=new LinkedHashMap<>();
        courseDetails.put("course",details.get("EducationList"));
        courseDetails.put("programCourses",details.get("EducationList"));
        course.add(courseDetails);
        jo.put("Course",course);

        JSONArray education = new JSONArray();
        LinkedHashMap educationDetails=new LinkedHashMap<>();
        educationDetails.put("schoolName",details.get("EducationList"));
        educationDetails.put("dateOfEntry",details.get("EducationList"));
        educationDetails.put("dateOfLeaving",details.get("EducationList"));
        education.add(educationDetails);
        jo.put("Education",education);
//6

        JSONParser parser = new JSONParser();
        JSONObject studentObject = (JSONObject) parser.parse(String.valueOf(civicaApplicationDetails));
        pushInformation("https://accountapi.lutonsfc.ac.uk/api/v1/","Application",jo);
        return jo;
    }
